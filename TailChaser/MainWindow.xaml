<Window x:Class="TailChaser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="800" Width="1050">
    <Grid>
        <Menu HorizontalAlignment="Left" Height="25" VerticalAlignment="Top" Width="1042">
            <MenuItem Header="File">
                <MenuItem Header="Open"></MenuItem>
            </MenuItem>
        </Menu>
        <DockPanel HorizontalAlignment="Left" Height="739" LastChildFill="False" Margin="0,30,0,0" VerticalAlignment="Top" Width="239"/>
        <RichTextBox HorizontalAlignment="Left" Height="729" Margin="244,30,0,0" VerticalAlignment="Top" Width="788">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>

    </Grid>
</Window>
